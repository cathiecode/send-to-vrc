{
  "name": "send-to-vrc",
  "private": true,
  "version": "1.2.6",
  "type": "module",
  "license": "MIT",
  "author": "cathiecode",
  "scripts": {
    "dev": "tauri dev",
    "dev:frontend": "vite",
    "tauri": "tauri",
    "build": "npm run generate:thirdparty && tauri build",
    "build:frontend": "tsc && vite build",
    "generate:thirdparty": "npm run generate:thirdparty:backend && npm run generate:thirdparty:frontend",
    "generate:thirdparty:backend": "cd src-tauri && cargo about generate -c misc/about.toml misc/about.hbs -o licenses/THIRDPARTY.backend",
    "generate:thirdparty:frontend": "npx generate-license-file --input package.json --output src-tauri/licenses/THIRDPARTY.frontend --overwrite",
    "check": "npm run check:frontend && npm run check:backend",
    "check:frontend": "prettier --check . && npx eslint src/ && tsc --noEmit",
    "check:backend": "cd src-tauri && cargo fmt --check && cargo clippy",
    "format": "npm run format:frontend && npm run format:backend",
    "format:frontend": "prettier --write .",
    "format:backend": "cd src-tauri && cargo fmt",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky && node scripts/prepare.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@formatjs/intl-localematcher": "^0.6.2",
    "@tanstack/query-core": "^5.90.2",
    "@tanstack/react-router": "^1.131.50",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-log": "^2.7.0",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@tauri-apps/plugin-os": "^2.3.1",
    "immer": "^10.2.0",
    "jotai": "^2.14.0",
    "jotai-tanstack-query": "^0.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-popper": "^2.3.0",
    "swr": "^2.3.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint/js": "^9.38.0",
    "@storybook/addon-a11y": "^9.1.6",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-onboarding": "^9.1.6",
    "@storybook/addon-vitest": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@tanstack/react-router-devtools": "^1.131.50",
    "@tanstack/router-plugin": "^1.131.50",
    "@tauri-apps/cli": "^2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "archiver": "^7.0.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "generate-license-file": "^4.1.0",
    "husky": "^9.1.7",
    "jotai-cache": "^0.5.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.92.1",
    "storybook": "^9.1.6",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.0.3",
    "vitest": "^3.2.4"
  }
}
